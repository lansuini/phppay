<div class="modal fade" id="modifyPwdModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body v-modal-row">
                <div class="form-group clearfix">
                    <label class="col-lg-2 control-label" for="modifyOldPwd">旧密码：</label>
                    <input type="password" class="form-control" id="modifyOldPwd" data-field="modifyOldPwd" placeholder="新密码" />
                </div>
                <div class="form-group clearfix">
                    <label class="col-lg-2 control-label" for="modifyNewPwd">新密码：</label>
                    <input type="password" class="form-control" id="modifyNewPwd" data-field="modifyNewPwd" placeholder="新密码" />
                </div>
                <div class="form-group clearfix">
                    <label class="col-lg-2 control-label" for="modifyRepNewPwd">重复新密码：</label>
                    <input type="password" class="form-control" id="modifyRepNewPwd" data-field="modifyRepNewPwd" placeholder="重复新密码" />
                </div>
                <div class="modal-footer">
                    <input type="hidden" id="modifyPwdAction" value=""  />
                    <input type="button" class="btn btn-default" value="关闭" data-dismiss="modal" />
                    <input onclick="javascript:common.btnModifyPwd(apiPath+'check/');" type="button" class="btn btn-primary" value="提交" id="btnModifyPwd" />
                </div>
            </div>
        </div>
    </div>
</div>
